{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"

    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "validate_certificates": false
    }
  },
  "vcap": {
    "services": {
      "local-mysql": [
        {
          "name": "myMySqlService",
          "credentials": {
            "uri": "mysql://root:@localhost:3306/steeltoe"
          }
        }
      ]
    }
  }
}
